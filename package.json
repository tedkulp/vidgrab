    "@types/bull": "^3.15.2",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/socket.io": "^3.0.2",
